{
  "name": "marmot-ts",
  "version": "0.0.1",
  "type": "module",
  "description": "Marmot protocol implmentation in TypeScript",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parres-hq/marmot-ts.git"
  },
  "keywords": [
    "nostr",
    "mls",
    "marmot"
  ],
  "author": "Parres",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/parres-hq/marmot-ts/issues"
  },
  "homepage": "https://github.com/parres-hq/marmot-ts#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.17"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf ./dist",
    "compile": "tsc -b tsconfig.build.json",
    "prepublishOnly": "npm run build",
    "format": "eslint --fix .",
    "lint": "eslint .",
    "test": "vitest",
    "prepare": "husky"
  },
  "dependencies": {
    "@hpke/core": "^1.7.4",
    "@noble/ciphers": "^2.0.1",
    "@noble/curves": "^2.0.1",
    "ts-mls": "^1.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/node": "^24.5.2",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "rimraf": "~6.0.0",
    "tsx": "^4.20.5",
    "typescript": "~5.9.0",
    "typescript-eslint": "^8.41.0",
    "vitest": "^3.2.4"
  }
}
